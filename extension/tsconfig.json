{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2016",
		"lib": ["es2016", "dom"],
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"outDir": "out",
		"rootDir": "src",
		"sourceMap": true,
		"esModuleInterop": true,
		"allowJs": true
	},
	"include": ["src",
		"src/Scg2ScsConverter.ts",
		"./src/Old2NewScgConverter.js"
	],
	"exclude": ["node_modules"]
}
