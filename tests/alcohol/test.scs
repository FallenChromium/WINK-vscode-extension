Adam
<- sc_node_class;
<- class;
=> nrel_main_idtf:
    [Adam] (*<-de;;*);
    [Адам] (*<-ru;;*);
    [Адам] (*<-uk;;*);
    [Adam] (*<-en;;*);
    [Adam] (*<-fr;;*);

//определение
<- rrel_key_sc_element: ... (*
	<- definition;;
    => nrel_main_idtf:
       [Biblische Figur] (*<-de;;*);
       [согласно Библии, первый человек на Земле] (*<-ru;;*);
       [в Старому Заповіті перша людина, батько роду людського] (*<-uk;;*);
       [first man according to the Abrahamic creation and religions such as Judaism, Christianity and Islam] (*<-en;;*);
       [premier humain, selon la Bible et le Coran] (*<-fr;;*);;
*);;

